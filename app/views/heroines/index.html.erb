<h2>All Heroines</h2>


<%= form_tag(powers_path method: :get) do %>
  Super Power <%= text_field_tag :power, params[:power] %>
  <%= submit_tag 'Search', name: nil %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
